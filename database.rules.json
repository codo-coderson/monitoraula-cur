{
  "rules": {
    "fixed_admins": {
      ".read": "root.child('fixed_admins').child(auth.uid).exists() || root.child('designated_admins').child(auth.uid).exists()",
      ".write": false
    },
    "designated_admins": {
      ".read": "root.child('fixed_admins').child(auth.uid).exists() || root.child('designated_admins').child(auth.uid).exists()",
      ".write": "root.child('fixed_admins').child(auth.uid).exists() || root.child('designated_admins').child(auth.uid).exists()"
    },
    "users": {
      ".read": "root.child('fixed_admins').child(auth.uid).exists() || root.child('designated_admins').child(auth.uid).exists()",
      ".write": "root.child('fixed_admins').child(auth.uid).exists() || root.child('designated_admins').child(auth.uid).exists()"
    },
    "clases": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "alumnos": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "registros": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "userPreferences": {
        ".read": "auth != null",
        ".write": "auth != null"
    }
  }
}